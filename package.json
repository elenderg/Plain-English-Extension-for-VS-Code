{
  "name": "plain-english-syntax-highlighter",
  "displayName": "Plain English Syntax Highlighter",
  "description": "Provides syntax highlighting for Plain English Programmin Language (from The Osmosian Order of Plain English Programmers -> www.osmosian.com)",
  "version": "0.0.2",
  "publisher": "OsmosianOrderofPlainEnglishProgrammers",
  "engines": {
    "vscode": "^1.70.1"
  },
  "categories": [
    "Programming Languages"
  ],
  "author": {
    "name": "Elender GÃ³is Gallas",
    "email": "help@osmosian.com"
  },
  "license": "UNLICENSE",
  "contributes": {
    "languages": [
      {
        "id": "plain",
        "aliases": [
          "Plain English",
          "plain"
        ],
        "extensions": [
          ".plain",
          ".pe",
          ".plainenglish",
          ".plain-english",
          "plain_english"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "plain",
        "scopeName": "source.plain",
        "path": "./syntaxes/plain.tmLanguage.json"
      }
    ]
  },
  "devDependencies": {
    "@vscode/test-cli": "^0.0.6",
    "@vscode/test-electron": "^2.3.9"
  },
  "repository": {
    "type" : "git",
    "url" : "https://github.com/elenderg/Plain-English-Extension-for-VS-Code.git"
  }
}
